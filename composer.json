  "type": "project",
  "license": "MIT",
  "bin": [
    "bin/seeder.php"
  ],
  "autoload": {
    "psr-4": {
      "Devian2011\\Seeder\\": "src/",
      "Devian2011\\Seeder\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Ilya Romanov",
      "email": "romanov.i.u@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "monolog/monolog": "2.*",
    "symfony/console": "5.4.*",
    "symfony/yaml": "^5.4",
    "symfony/expression-language": "^5.4",
    "symfony/dotenv": "^5.4",
    "fakerphp/faker": "^1.19",
    "ext-json": "*"
